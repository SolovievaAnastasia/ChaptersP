#include "stdafx.h"
#include <iostream>
using namespace std;

int main()
{
	double k1, k2;
	cout << "First enter kol-vo litr" << endl;
	cin >> k1;
	cout << "Second enter kol km" << endl;
	cin >> k2;
	double con1 = 1.609;
		double con2 = 0.26417;
	cout << "finaly " <<( k2/con1)/(k1*con2) << " mile/gallon" << endl;
	return 0;
}
